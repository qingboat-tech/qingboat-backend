<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.qingboat.uc.dao.UserDao">
    
    <select id="getUserById" parameterType="java.lang.Long" resultType="com.qingboat.uc.entity.UserEntity">
        select id,user_name AS userName,birthday,sex,address from user where id =#{id}
    </select>

</mapper>