<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.qingboat.uc.dao.UserProfileDao">
    <select id="findByUserId" parameterType="long" resultType="com.qingboat.uc.entity.UserProfileEntity">
        select * from apps_userprofile where user_id = #{userId}
    </select>
</mapper>